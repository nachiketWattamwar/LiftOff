<head>
  <% include ../partials/head %>
  <% include ../partials/logout %>
  <link href="/projects.css" rel="stylesheet" type="text/css" />
  <meta name="viewport" content="width=device-width, initial-scale=1">

</head>
<body>
  <canvas id="canvas"></canvas>
  <div class="container" id="containerID">
  <div class="sapce-agency-title-div">
    <div class="shadow"></div>
    <img src="https://www.nasa.gov/sites/default/files/thumbnails/image/nasa-logo-web-rgb.png">
  </div>

  <div class="container">
    <% for(var i=0; i < data.length; i++) { %>
    <div class="row">
      <div class="col-md-12">
        <div class="row">
          <div class="col-md-4">
            <img
              class="space-img"
              src="<%= data[i].image_url %>"
              alt=""4085959537
            />
          </div>
          <div class="col-md-8" id="imageID">
            <h3 class="space-title">
              <%= data[i].name %>
            </h3>
            <h4 class="space-time">
              <%= data[i].start_date %> - <%= data[i].end_date %>
            </h4>
            <p class="space-desc"><%= data[i].description %></p>
          </div>
        </div>
      </div>
    </div>
    <div class="separator"></div>
    <% } %>
  </div>
  </div>



  <script type="text/javascript" src="/canvas.js"></script>
</body>
